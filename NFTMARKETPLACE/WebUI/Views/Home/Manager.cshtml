@using WebUI.Models.Helpers
<script src="~/Scripts/Views/ManagerUserTable.js"></script>
<script src="~/Scripts/Views/ManagerCollectionTable.js"></script>
<script src="~/Scripts/Views/ManagerCategoryTable.js"></script>
<script src="~/Scripts/Views/ManagerPriceTable.js"></script>
<script src="~/Scripts/Views/ManagerNFTTable.js"></script>
@{
    ViewBag.Title = "Manager";
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <title>NETMARKETPLACE - By NetQuality</title>
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content=" " />
    <meta name="keywords" content="" />
    <meta content="Pichforest" name="author" />

    <link rel="shortcut icon" href="../../Content/images/HeadIcon.png">

    <!-- Bootstrap css -->
    <link rel="stylesheet" href="../../css/bootstrap.min.css" type="text/css" id="bootstrap-style" />

    <!-- Material Icon Css -->
    <link rel="stylesheet" href="../../css/materialdesignicons.min.css" type="text/css" />

    <!-- Unicon Css -->
    @*   <link rel="stylesheet" href="https://unicons.iconscout.com/release/v4.0.0/css/line.css" />
    *@

    <!-- Swiper Css -->
    <link rel="stylesheet" href="../../css/tiny-slider.css" type="text/css" />
    <link rel="stylesheet" href="../../css/swiper.min.css" type="text/css" />

    <!-- Custom Css -->
    <link rel="stylesheet" href="../../css/style.min.css" type="text/css" />

</head>

<body data-bs-spy="scroll" data-bs-target="#navbar-navlist" data-bs-offset="71">

    <!-- light-dark mode button -->
    <a href="javascript: void(0);" id="mode" class="mode-btn text-white rounded-end">
        <i class="uil uil-brightness mode-dark mx-auto"></i>
        <i class="uil uil-moon bx-spin mode-light"></i>
    </a>

    <!--Navbar Start-->
    <nav class="navbar navbar-expand-lg fixed-top sticky" id="navbar">
        <div class="container-fluid custom-container">
            <a class="navbar-brand logo f-30 text-dark fw-bold" href="index.html">
                <img src="../../Content/images/NETQUALITY.png" class="logo-dark" alt="" height="40">
                <img src="../../Content/images/NETQUALITY.png" class="logo-light" alt="" height="40">
            </a>
            <div>
                <button class="navbar-toggler me-3" type="button" data-bs-toggle="collapse" data-bs-target="#navbarCollapse" aria-controls="navbarCollapse" aria-label="Toggle navigation">
                    <i class="mdi mdi-menu"></i>
                </button>
            </div>
            <div class="collapse navbar-collapse" id="navbarCollapse">
                <ul class="navbar-nav mx-auto navbar-center">
                    <li class="nav-item dropdown dropdown-hover">
                        @Html.ActionLink("Home", "Index", "Home", new { @class = "nav-link" })


                    </li>

                    <li class="nav-item">
                        @Html.ActionLink("Marketplace", "Marketplace", "Marketplace", new { @class = "nav-link" })
                    </li>

                    <li class="nav-item">

                        @Html.ActionLink("Collection", "Collection", "Collection", new { @class = "nav-link" })

                    </li>


                    <li class="nav-item">

                        @Html.ActionLink("NFT'S", "ItemDetails", "NFT", new { @class = "nav-link" })


                    </li>


                    <li class="nav-item">
                        @Html.ActionLink("Login", "Login", "Login", new { @class = "nav-link" })

                    </li>


                    <li class="nav-item dropdown dropdown-hover">
                        @Html.ActionLink("Register", "Register", "Registration", new { @class = "nav-link dropdown-toggle" })

                        <ul class="dropdown-menu dropdown-menu-center" aria-labelledby="homedrop">
                            <li>

                                @Html.ActionLink("Register", "Register", "Register", new { @class = "dropdown-item" })

                            </li>
                            <li>

                                @Html.ActionLink("As content creator", "RegisterContentCreator", "RegisterContentCreator", new { @class = "dropdown-item" })
                            </li>
                        </ul>
                    </li>


                    <!--end dropdown-->


                    <li class="nav-item dropdown dropdown-hover">

                        @Html.ActionLink("Profile", "Profile", "Profile", new { @class = "nav-link dropdown-toggle" })
                        <ul class="dropdown-menu dropdown-menu-center" aria-labelledby="homedrop">
                            <li>

                                @Html.ActionLink("Profile", "Profile", "Profile", new { @class = "dropdown-item" })
                            </li>
                            <li>

                                @Html.ActionLink("EditProfile", "EditProfile", "EditProfile", new { @class = "dropdown-item" })
                            </li>
                        </ul>
                    </li>


                    <li class="nav-item">
                        @Html.ActionLink("Contact", "Contact", "Contact", new { @class = "nav-link" })

                    </li>
                    <!--end dropdown-->
                </ul>
                <!--end navbar-nav-->
                @Html.ActionLink("Connect to Wallet", "Wallet", "Wallet", new { @class = "btn btn-primary" })


            </div>

        </div>
        <!--end container-->
    </nav>

    <!-- Navbar End -->
    <!-- start section -->
    <section class="ranking section bg-light">
        <!-- start container -->
        <div class="container">
            <div class="row mt-4">
                <div class="col-lg-12">
                    <nav aria-label="breadcrumb">
                        <ol class="breadcrumb default mb-0">
                            <li class="breadcrumb-item">
                                <a href="index.html" class="text-muted">Home</a>
                            </li>
                            <li class="breadcrumb-item active text-primary" aria-current="page">Ranking</li>
                        </ol>
                    </nav>
                </div>
            </div>

            <div class="row align-items-center justify-content-center">
                <div class="col-lg-8">
                    <div class="inner-heading text-center">
                        <div class="mt-4">
                            <h1 class="fw-bold">Dashboard</h1>
                            <P class="text-muted">Welcome to your dashboard! </P>
                        </div>
                    </div>

                    <div class="heading-bottom-icon d-flex justify-content-center text-center">
                        <i class="mdi mdi-image-filter-vintage"></i>
                        <i class="mdi mdi-image-filter-vintage mx-2"></i>
                        <i class="mdi mdi-image-filter-vintage"></i>
                    </div>
                </div>
            </div>


            <div class="row align-items-center mt-5">
                <div class="col-lg-12">
                    <ul class="nav nav-pills align-items-center mb-3" id="pills-tab" role="tablist">
                        <li class="nav-item" role="presentation">
                            <button class="nav-link active f-20" id="pills-games-tab" data-bs-toggle="pill" data-bs-target="#pills-games" type="button" role="tab" aria-controls="pills-games" aria-selected="true">
                                <i class="mdi mdi-account f-24 align-middle"></i>
                                Users
                            </button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="nav-link f-20" id="pills-art-tab" data-bs-toggle="pill" data-bs-target="#pills-art" type="button" role="tab" aria-controls="pills-art" aria-selected="false"><i class="mdi mdi-image-filter-none f-24 align-middle"></i> Collections</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="nav-link f-20" id="pills-tranding-tab" data-bs-toggle="pill" data-bs-target="#pills-tranding" type="button" role="tab" aria-controls="pills-tranding" aria-selected="false">
                                <i class="mdi mdi-image-area f-24 align-middle"></i>
                                NFTs
                            </button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="nav-link f-20" id="pills-music-tab" data-bs-toggle="pill" data-bs-target="#pills-music" type="button" role="tab" aria-controls="pills-music" aria-selected="false"><i class="mdi mdi-music f-24 align-middle"></i> Category</button>
                        </li>
                        <li class="nav-item" role="presentation">
                            <button class="nav-link f-20" id="pills-domain-tab" data-bs-toggle="pill" data-bs-target="#pills-domain" type="button" role="tab" aria-controls="pills-domain" aria-selected="false"><i class="mdi mdi-cash f-24 align-middle"></i> Prices</button>
                        </li>
                    </ul>
                    <div class="tab-content mt-5" id="pills-tabContent">
                        <div class="tab-pane fade show active" id="pills-games" role="tabpanel" aria-labelledby="pills-games-tab">
                            @Html.CtrlTable("ManagerUserTable", "userList", "Lista de usuarios", "Identification,Name,Email,Status", "Cedula,Name,Email,Status", "Prueba")
                            @Html.CtrlButton("ManagerUserTable", "StatusButton", "Cambiar estado", "StatusUser")
                        </div>
                        <div class="tab-pane fade" id="pills-art" role="tabpanel" aria-labelledby="pills-art-tab">
                            @Html.CtrlTable("ManagerCollectionTable", "collectionList", "Lista de colecciones", "ID,Collection Name,ID Company,Category", "Id,CollectionName,CompanyId,CategoryName", "Prueba")
                        </div>
                        <div class="tab-pane fade" id="pills-tranding" role="tabpanel" aria-labelledby="pills-tranding-tab">
                            @Html.CtrlTable("ManagerNFTTable", "nftList", "Lista de NFTs", "Name,Price,Creator,Owner,Category", "NftName,Price,IdCreator,IdOwner,CategoryName", "Prueba")
                        </div>
                        <div class="tab-pane fade" id="pills-music" role="tabpanel" aria-labelledby="pills-music-tab">
                            <div style="display: flex; align-items: flex-end; margin-bottom: 20px;">
                                <form id="categoryForm" style="margin-right: 15px;">
                                    @Html.CtrlInput("txtCategoryName", "text", "Category name:", "Enter the category name", "CategoryName")
                                </form>
                                <div class="submit-button mt-4">
                                    @Html.CtrlButton("ManagerCategoryTable", "btnNewCategory", "Create", "CreateCategory", "primary w - 100")
                                    @Html.CtrlButton("ManagerCategoryTable", "btnDeleteCategory", "Delete", "DeleteCategory", "primary w - 100")
                                </div>
                            </div>
                            <div>
                                @Html.CtrlTable("ManagerCategoryTable", "categoryList", "Lista de categoria", "ID,Category Name", "Id,CategoryName", "Prueba")
                            </div>
                        </div>
                        <div class="tab-pane fade" id="pills-domain" role="tabpanel" aria-labelledby="pills-domain-tab">
                            <div style="display: flex; align-items: flex-end; margin-bottom: 20px;">
                                <form id="priceForm" style="margin-right: 15px;">
                                    @Html.CtrlInput("txtPriceAmount", "text", "Price amount:", "Enter the price", "Amount")
                                </form>
                                <div class="submit-button mt-4">
                                    @Html.CtrlButton("ManagerPriceTable", "btnUpdPrice", "Update price", "UpdatePrice", "primary w - 100")                              
                                </div>
                            </div>
                            <div>
                                @Html.CtrlTable("ManagerPriceTable", "priceList", "Lista de precios", "ID,Price,Type", "Id,Amount,Type", "Prueba")
                            </div>
                        </div>
                        <div class="tab-pane fade" id="pills-memes" role="tabpanel" aria-labelledby="pills-memes-tab">
                            <div class="table-responsive">
                                <table class="table table-borderless">
                                    <thead>
                                        <tr class="table-title">
                                            <th scope="col">Collection</th>
                                            <th scope="col">Volume</th>
                                            <th scope="col">24 H %</th>
                                            <th scope="col">1 Week %</th>
                                            <th scope="col">Owners</th>
                                            <th scope="col">Employe</th>
                                            <th scope="col">Floor Price</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr class="rank-profile">
                                            <th scope="row">
                                                <div class="d-flex align-items-center">
                                                    <div class="flex-shrink-0">
                                                        <div class="avatar position-relative">
                                                            <img src="../../Content/images/inner-image/ranking/img-5.png" alt="" class=" avatar-lg rounded">

                                                            <span class="number position-absolute bottom-0 start-100 translate-middle-x p-2 bg-primary border border-light rounded-circle">1</span>
                                                        </div>
                                                    </div>
                                                    <div class="flex-grow-1 ms-3">
                                                        <p class="mb-0 fw-bold">Creative Game collection</p>
                                                    </div>
                                                </div>
                                            </th>
                                            <td>21, 1241</td>
                                            <td class="text-success">+1245%</td>
                                            <td class="text-danger">-210%</td>
                                            <td>1.2K</td>
                                            <td>20K</td>
                                            <td>12, 3211 CFC</td>
                                        </tr>
                                        <tr class="rank-profile">
                                            <th scope="row">
                                                <div class="d-flex align-items-center">
                                                    <div class="flex-shrink-0">
                                                        <div class="avatar position-relative">
                                                            <img src="../../Content/images/inner-image/ranking/img-6.png" alt="" class=" avatar-lg rounded">

                                                            <span class="number position-absolute bottom-0 start-100 translate-middle-x p-2 bg-primary border border-light rounded-circle">2</span>
                                                        </div>
                                                    </div>
                                                    <div class="flex-grow-1 ms-3">
                                                        <p class="mb-0 fw-bold">Colorful Abstract Painting</p>
                                                    </div>
                                                </div>
                                            </th>
                                            <td>21, 3214</td>
                                            <td class="text-success">+6212%</td>
                                            <td class="text-danger">-512%</td>
                                            <td>1.4K</td>
                                            <td>51K</td>
                                            <td>21, 4512 CFC</td>
                                        </tr>
                                        <tr class="rank-profile">
                                            <th scope="row">
                                                <div class="d-flex align-items-center">
                                                    <div class="flex-shrink-0">
                                                        <div class="avatar position-relative">
                                                            <img src="../../Content/images/inner-image/ranking/img-7.png" alt="" class=" avatar-lg rounded">

                                                            <span class="number position-absolute bottom-0 start-100 translate-middle-x p-2 bg-primary border border-light rounded-circle">3</span>
                                                        </div>
                                                    </div>
                                                    <div class="flex-grow-1 ms-3">
                                                        <p class="mb-0 fw-bold">The girl with the firefly</p>
                                                    </div>
                                                </div>
                                            </th>
                                            <td>45, 5245</td>
                                            <td class="text-success">+3841%</td>
                                            <td class="text-danger">-230%</td>
                                            <td>3.2K</td>
                                            <td>26K</td>
                                            <td>29, 6911 CFC</td>
                                        </tr>
                                        <tr class="rank-profile">
                                            <th scope="row">
                                                <div class="d-flex align-items-center">
                                                    <div class="flex-shrink-0">
                                                        <div class="avatar position-relative">
                                                            <img src="../../Content/images/inner-image/ranking/img-8.png" alt="" class=" avatar-lg rounded">

                                                            <span class="number position-absolute bottom-0 start-100 translate-middle-x p-2 bg-primary border border-light rounded-circle">4</span>
                                                        </div>
                                                    </div>
                                                    <div class="flex-grow-1 ms-3">
                                                        <p class="mb-0 fw-bold">Dodo hide the seek</p>
                                                    </div>
                                                </div>
                                            </th>
                                            <td>21, 1285</td>
                                            <td class="text-success">+6854%</td>
                                            <td class="text-danger">-320%</td>
                                            <td>2.5K</td>
                                            <td>50K</td>
                                            <td>32, 5387 CFC</td>
                                        </tr>
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </div>
        <!-- end container -->
    </section>

    <!-- start footer -->
    <section class="footer">
        <div class="container">
            <div class="top-footer">
                <div class="row">
                    <div class="col-lg-4">
                        <div class="footer-info mt-4">
                            <!-- <img src="../../Content/images/inner-image/logo-dark.png" alt="" height="22"> -->
                            <a class="logo f-30 text-dark fw-bold" href="index.html">
                                <img src="../../Content/images/NETQUALITY.png" class="logo-dark" alt="" height="40">
                                <img src="../../Content/images/NETQUALITY.png" class="logo-light" alt="" height="40">
                            </a>
                            <p class="f-18 mt-2 text-muted">
                                Business opportunities Are Like <br> Buses, There's Always Another <br> One Coming...
                            </p>
                        </div>
                    </div>

                    <div class="col-lg-5">
                        <div class="row">

                            <div class="col-lg-6">
                                <div class="mt-4">
                                    <h5 class="f-18 fw-bold">Policies</h5>
                                    <ul class="list-unstyled footer-link mt-3">
                                        <li><a href="">Security & Provciy</a></li>
                                        <li><a href="">Marketplace</a></li>
                                        <li><a href="">Term & Condition</a></li>
                                        <li><a href="">Collection</a></li>
                                    </ul>
                                </div>
                            </div>

                            <div class="col-lg-6">
                                <div class="mt-4">
                                    <h5 class="f-18 fw-bold">Company</h5>
                                    <ul class="list-unstyled footer-link mt-3">
                                        <li><a href="index.html">Home</a></li>
                                        <li><a href="activity.html">Pages</a></li>
                                        <li><a href="blog.html">Blog</a></li>
                                        <li><a href="contact.html">Contact</a></li>
                                    </ul>
                                </div>
                            </div>
                        </div>
                    </div>

                    <div class="col-lg-3">
                        <div class="mt-4 pl-0 pl-lg-4">
                            <h5 class="f-18 fw-bold">Follow Us</h5>
                            <ul class="list-inline social-icons-list pt-3">
                                <li class="list-inline-item">
                                    <a href="#"><i class="mdi mdi-facebook"></i></a>
                                </li>
                                <li class="list-inline-item">
                                    <a href="#"><i class="mdi mdi-twitter"></i></a>
                                </li>
                                <li class="list-inline-item">
                                    <a href="#"><i class="mdi mdi-linkedin"></i></a>
                                </li>
                                <li class="list-inline-item">
                                    <a href="#"><i class="mdi mdi-google-plus"></i></a>
                                </li>
                            </ul>
                        </div>
                    </div>
                </div>
            </div>


            <hr>


        </div>
    </section>
    <!-- end footer -->
    <!--Bootstrap Js-->
    <script src="../../Scripts/bootstrap.bundle.min.js"></script>

    <!-- Slider Js -->
    <script src="../../Scripts/tiny-slider.js"></script>
    <script src="../../Scripts/swiper.min.js"></script>

    <!-- counter -->
    <script src="../../Scripts/counter.init.js"></script>

    <!-- App Js -->
    <script src="../../Scripts/app.js"></script>


</body>

</html>
